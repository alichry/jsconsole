{
  "name": "@remy/jsconsole",
  "version": "2.1.2",
  "now": {
    "alias": "2.jsconsole.com"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "printWidth": 80,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "dependencies": {
    "@preact/compat": "^16.9.0",
    "babylon": "^6.18.0",
    "babylon-walk": "^1.0.2",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "is-url": "^1.2.4",
    "lodash": "^4.17.4",
    "preact": "^10.26.5",
    "prop-types": "^15.5.10",
    "react": "^16.14.0",
    "react-copy-to-clipboard": "^5.0.0",
    "react-dom": "^16.14.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "whatwg-fetch": "2.0.3",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^10.1.0",
    "body-parser": "^1.17.2",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "1.1.3",
    "css-loader": "^7.1.2",
    "dotenv": "4.0.0",
    "eslint": "^8.57.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-webpack-plugin": "^5.0.1",
    "file-loader": "^6.2.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "node-uuid": "^1.4.8",
    "object-assign": "4.1.1",
    "path-browserify": "^1.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "prettier-eslint": "^8.7.4",
    "promise": "7.1.1",
    "react-dev-utils": "^12.0.1",
    "react-error-overlay": "^6.1.0",
    "redbox-react": "^1.5.0",
    "redux-devtools": "^3.4.0",
    "react-test-renderer": "^16.14.0",
    "style-loader": "^4.0.0",
    "url": "^0.11.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-dev-server": "^5.2.1",
    "webpack-manifest-plugin": "^5.0.1"
  },
  "_proxy": "http://localhost:3001",
  "scripts": {
    "env": "env",
    "start": "REACT_APP_VERSION=$npm_package_version node scripts/start.js",
    "build": "rm -rf build; PUBLIC_URL=/ REACT_APP_VERSION=$npm_package_version node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "styles": "cat src/*.css > src/core/jsconsole.css",
    "watch": "nodemon -w src/ -e css -x 'npm run styles'",
    "lib": "rm -rf dist; npm run styles; NODE_ENV=production babel src/core --out-dir dist --copy-files --ignore '__tests__'",
    "deploy": "npm run build && s3_website push",
    "prepare": "npm run lib"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "object-assign"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ]
  },
  "__eslintConfig": {
    "extends": "react-app"
  },
  "description": "Web based console - for presentations, workshops and jsbin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remy/jsconsole.git"
  },
  "keywords": [],
  "author": "Remy Sharp <remy@leftlogic.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remy/jsconsole/issues"
  },
  "homepage": "https://github.com/remy/jsconsole#readme"
}
